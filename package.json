{
  "name": "variants",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev --host",
    "build": "astro build",
    "preview": "dotenv -e .env.production.local -- deno run --allow-net --allow-read --allow-env ./dist/server/entry.mjs",
    "test:dev": "dotenv -e .env -- vitest run",
    "astro": "astro",
    "prisma:dev": "rm -rf node_modules/.prisma; prisma generate && mkdir -p prisma/generated/deno && touch prisma/generated/deno/index.js",
    "prisma:prod": "rm -rf node_modules/.prisma; prisma generate --no-engine; PRISMA_OUTPUT=./generated deno run -A --node-modules-dir=false npm:prisma@5.14.0 generate --no-engine; cd prisma/generated/deno; cp edge.ts index.js",
    "postinstall:dev": "pnpm prisma:dev",
    "postinstall:prod": "pnpm prisma:prod && pnpm supa:gen:prod",
    "postinstall": "pnpm postinstall:${VW_ENV:-dev}",
    "supa:start": "supabase start",
    "supa:gen:dev": "supabase gen types typescript --local > src/db/supabase/types.ts && eslint src/db/supabase/types.ts --fix",
    "supa:gen:prod": "supabase gen types typescript --schema public --db-url \"$DB_URL\" > src/db/supabase/types.ts && eslint src/db/supabase/types.ts --fix",
    "supa:gen": "pnpm supa:gen:${VW_ENV:-dev}",
    "supa:stop": "supabase stop",
    "supa": "pnpm supa:start && pnpm supa:gen",
    "migrate:production": "dotenv -e .env.production.local -- prisma migrate deploy",
    "migrate:dev": "prisma migrate deploy",
    "deploy": "pnpm run build && dotenv -e .env.production.local -- deployctl deploy -p variants-world --prod --include dist ./dist/server/entry.mjs"
  },
  "dependencies": {
    "@astrojs/preact": "^3.3.0",
    "@deno/astro-adapter": "^0.1.2",
    "@preact/signals": "^1.2.3",
    "@prisma/client": "5.14.0",
    "@prisma/extension-accelerate": "^1.1.0",
    "@supabase/supabase-js": "^2.43.4",
    "astro": "^4.9.2",
    "astro-windi": "^0.0.2",
    "base64url": "^3.0.1",
    "dotenv": "^16.4.5",
    "hash-wasm": "^4.11.0",
    "jose": "^5.3.0",
    "preact": "^10.22.0",
    "react-device-detect": "^2.2.3",
    "ts-node": "^10.9.2",
    "typescript": "5.5.1-rc",
    "ulidx": "^2.3.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.44.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "prettier": "^3.2.5",
    "prisma": "5.14.0",
    "sass": "^1.77.2",
    "supabase": "^1.169.8",
    "vitest": "^1.6.0"
  }
}
