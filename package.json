{
  "name": "variants",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:prod": "dotenv -e .env.production.local -- astro dev --mode prod",
    "start": "astro dev --host",
    "build": "dotenv -e .env.production.local -- astro build",
    "preview": "dotenv -e .env.production.local -- deno run --allow-net --allow-read --allow-env --unstable-kv --no-lock ./dist/server/entry.mjs",
    "test:dev": "dotenv -e .env -- vitest run",
    "astro": "astro",
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy",
    "convex:generate": "convex dev --once",
    "migrate:to-convex": "tsx scripts/migrate-to-convex.ts",
    "postinstall": "convex dev --once || true",
    "deploy": "pnpm run build && dotenv -e .env.production.local -- deployctl deploy -p variants-world --prod --include dist ./dist/server/entry.mjs"
  },
  "dependencies": {
    "@astrojs/preact": "^3.5.4",
    "@convex-dev/presence": "^0.3.0",
    "@deno/astro-adapter": "^0.1.5",
    "@dino/github-webhook": "npm:@jsr/dino__github-webhook@^0.0.6",
    "@preact/signals": "^1.3.1",
    "@prisma/client": "5.14.0",
    "@prisma/extension-accelerate": "^1.1.0",
    "@supabase/supabase-js": "^2.46.2",
    "astro": "^4.16.16",
    "astro-windi": "^0.0.2",
    "base64url": "^3.0.1",
    "convex": "^1.31.2",
    "dotenv": "^16.4.5",
    "hash-wasm": "^4.12.0",
    "jose": "^5.9.6",
    "preact": "^10.25.0",
    "react-device-detect": "^2.2.3",
    "ts-node": "^10.9.2",
    "typescript": "5.7.2",
    "ulidx": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8.44.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "prettier": "^3.4.1",
    "prisma": "5.14.0",
    "sass": "^1.81.0",
    "supabase": "^2.0.0",
    "tsx": "^4.21.0",
    "vitest": "^2.1.6"
  }
}
