{
  "name": "variants",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "dotenv -e .env.development -- astro dev",
    "start": "astro dev --host",
    "build": "astro build",
    "preview": "dotenv -- deno run --allow-net --allow-read --allow-env ./dist/server/entry.mjs",
    "astro": "astro",
    "gen:prisma": "rm -rf node_modules/.prisma; dotenv -e .env.development -- prisma generate --no-engine; PRISMA_OUTPUT=./generated deno run -A --unstable npm:prisma@5.4.1 generate --no-engine; cd prisma/generated/deno; cp edge.ts index.js",
    "gen:supabase": "supabase gen types typescript --project-id $(dotenv -p SUPABASE_PROJECT_ID) --schema public > ./src/db/supabase/types.ts; eslint --fix ./src/db/supabase/types.ts",
    "postinstall": "npm run gen:prisma",
    "deploy": "npm run build; dotenv -- deployctl deploy -p variants-world --prod --include dist ./dist/server/entry.mjs;"
  },
  "dependencies": {
    "@astrojs/deno": "^5.0.1",
    "@astrojs/preact": "^3.0.0",
    "@prisma/client": "^5.4.1",
    "@prisma/extension-accelerate": "^0.6.2",
    "@supabase/supabase-js": "^2.26.0",
    "@types/node": "^20.4.2",
    "@types/ua-parser-js": "^0.7.39",
    "astro": "^3.2.3",
    "astro-windi": "^0.0.2",
    "base64url": "^3.0.1",
    "dotenv": "^16.3.1",
    "hash-wasm": "^4.11.0",
    "jose": "^5.1.1",
    "preact": "^10.16.0",
    "react-device-detect": "^2.2.3",
    "react-query": "^3.39.3",
    "ts-node": "^10.9.1",
    "ua-parser-js": "^1.0.37",
    "ulidx": "^2.2.1",
    "valibot": "^0.13.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.44.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "prettier": "^3.0.2",
    "prisma": "^5.4.1",
    "sass": "^1.69.0",
    "supabase": "^1.100.1"
  }
}
