{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "assets/*": ["src/assets/*"],
      "cgabot": ["src/cgabot"],
      "cgabot/*": ["src/cgabot/*"],
      "components/*": ["src/components/*"],
      "db/*": ["src/db/*"],
      "layouts/*": ["src/layouts/*"],
      "pages/*": ["src/pages/*"],
      "services/*": ["src/services/*"],
      "utils/*": ["src/utils/*"],
    }
  }
}
